{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"slicemachine": "start-slicemachine"
	},
	"dependencies": {
		"@fontsource-variable/merriweather": "^5.2.6",
		"@fontsource-variable/urbanist": "^5.2.7",
		"@fontsource/pt-serif": "^5.2.8",
		"@prismicio/client": "^7.21.0",
		"@prismicio/svelte": "^2.0.0",
		"@tailwindcss/vite": "^4.1.17",
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"clsx": "^2.1.1",
		"gsap": "^3.14.1",
		"resend": "^6.6.0",
		"tailwindcss": "^4.1.17",
		"three": "^0.181.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@iconify-json/fa-brands": "^1.2.2",
		"@iconify-json/ic": "^1.2.4",
		"@iconify-json/material-symbols": "^1.2.49",
		"@slicemachine/adapter-sveltekit": "^0.3.89",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.21.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/typography": "^0.5.19",
		"@types/eslint": "^9.6.1",
		"concurrently": "^9.1.2",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"slice-machine-ui": "^2.20.2",
		"svelte": "^5.33.14",
		"svelte-check": "^4.2.1",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1",
		"unplugin-icons": "^22.5.0",
		"vite": "^6.3.5"
	}
